{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "overrides": [
    {
      "files": ["migrations/*.js"],
      "rules": {
        "no-unused-vars": "off"
      }
    }
  ]
}
